{
  "name": "svelte-nwjs-template",
  "version": "0.0.3",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "@tsconfig/svelte": "^3.0.0",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "nwjs": "^1.4.4",
    "nwjs-builder-phoenix": "^1.15.0",
    "rollup-plugin-css-porter": "^1.0.2",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^2.0.1",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-svelte-hot": "^1.0.0-8",
    "rollup-plugin-svelte-static-html": "^0.1.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.7"
  },
  "scripts": {
    "rename": "ts-node-esm tools/rename.ts",
    "set-version": "ts-node-esm tools/set-version.ts",
    "set-copyright": "ts-node-esm tools/set-copyright.ts",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "vite build --watch",
    "preview": "vite preview",
    "build": "cross-env NODE_ENV=production vite build",
    "start": "run dist",
    "dist-win32": "build --tasks win-x86 dist",
    "dist-win64": "build --tasks win-x64 dist",
    "dist-linux32": "build --tasks linux-x86 dist",
    "dist-linux64": "build --tasks linux-x64 dist",
    "dist-mac64": "build --tasks mac-x64 dist",
    "dist": "npm-run-all --parallel dist-win32 dist-win64 dist-linux32 dist-linux64 dist-mac64",
    "bundle-win32": "ts-node-esm tools/bundle.ts win32",
    "bundle-win64": "ts-node-esm tools/bundle.ts win64",
    "bundle-mac64": "ts-node-esm tools/bundle.ts mac64",
    "bundle-lin32": "ts-node-esm tools/bundle.ts lin32",
    "bundle-lin64": "ts-node-esm tools/bundle.ts lin64",
    "bundle": "npm-run-all --parallel bundle-win32 bundle-win64 bundle-mac64 bundle-lin32 bundle-lin64"
  },
  "dependencies": {
    "svelte-routing": "^1.6.0"
  }
}
