{
  "name": "svelte-nwjs-template",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "@tsconfig/svelte": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "mini-css-extract-plugin": "^2.6.1",
    "nw-dev": "^3.0.1",
    "nwjs": "^1.4.4",
    "nwjs-builder-phoenix": "^1.15.0",
    "rollup-plugin-svelte-static-html": "^0.1.0",
    "serve": "^14.0.1",
    "style-loader": "^3.3.1",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-loader": "^3.1.3",
    "svelte-preprocess": "^4.10.7",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.7",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "rename": "ts-node-esm tools/rename.ts",
    "set-version": "ts-node-esm tools/set-version.ts",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dev": "vite build --watch",
    "preview": "vite preview",
    "test": "npm-run-all --parallel dev preview",
    "build": "vite build",
    "start": "run --remote-debugging-port=9222 --x64 --mirror https://dl.nwjs.io/ dist",
    "dist": "build --tasks win-x86,win-x64,linux-x86,linux-x64,mac-x64 --mirror https://dl.nwjs.io/ --concurrent dist",
    "bundle": "npm-run-all build dist"
  }
}